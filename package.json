{
  "name": "paperlib-paper-locate-extension",
  "version": "1.0.0",
  "description": "This extension is for locating paper files on internet for Paperlib",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "prebuild": "tsc --noEmit",
    "build": "tsc --noEmit && vite build"
  },
  "engines": {
    "node": ">=16.17.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/node": "^20.10.4",
    "@types/tough-cookie": "^4.0.5",
    "bson-objectid": "^2.0.4",
    "mathml-to-latex": "^1.3.0",
    "node-html-parser": "^6.1.11",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rollup-plugin-hypothetical": "^2.1.1",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-modify": "^3.0.0",
    "string-similarity": "^4.0.4",
    "typescript": "^5.1.3",
    "vite": "4.3.7",
    "vitest": "0.24.3"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_HOSTNAME": "127.0.0.1",
      "VITE_DEV_SERVER_PORT": 3344,
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "keywords": [
    "academic",
    "paper",
    "management",
    "paperlib"
  ]
}